{
	"name": "rust_lambda-infrastructure",
	"version": "0.1.0",
	"bin": {
		"cdk": "bin/cdk.js"
	},
	"scripts": {
		"build": "npm run lint && npm run test && npm run synth",
		"compile": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"lint": "eslint . --ext .ts",
		"synth": "cdk synth",
		"deploy": "npm run deploy:databaseStack && npm run deploy:rustLambdaStack",
		"deploy:databaseStack": "cdk deploy DatabaseStack -- --verbose --ci --require-approval never",
		"deploy:rustLambdaStack": "cdk deploy RustLambdaStack -- --verbose --ci --require-approval never"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "20.12.5",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"aws-cdk": "2.136.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.4"
	},
	"dependencies": {
		"aws-cdk-lib": "2.136.0",
		"cargo-lambda-cdk": "^0.0.20",
		"constructs": "^10.3.0",
		"source-map-support": "^0.5.21"
	}
}
